{
  "extends": [
      "config:base",
      ":disableDependencyDashboard"
  ],
  "addLabels": [
      "type: dependencies"
  ],
  "groupName": "all dependencies",
  "groupSlug": "all",
  "packageRules": [
      {
          "automerge": true,
          "groupName": "all dependencies",
          "groupSlug": "all",
          "matchPackagePatterns": [
              "*"
          ],
          "matchUpdateTypes": [
              "minor",
              "patch",
              "pin",
              "digest"
          ],
          "ignoreDeps": ["k8s.io/client-go"]
      }
  ],
  "semanticCommits": "enabled",
  "separateMajorMinor": false
}